{
    "name": "ardupilot-sitl",
    "remoteUser": "dockeruser",
    "dockerComposeFile": "${localWorkspaceFolder}/docker-compose.yml",
    "service": "ardupilot-sitl",
    "workspaceFolder": "/",
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.python",
                "charliermarsh.ruff",
                "ms-vscode.cpptools-extension-pack",
                "redhat.vscode-xml",
                "tamasfe.even-better-toml",
                "yzhang.markdown-all-in-one",
            ]
        }
    }
}