{
  "name": "ROS 2 Workspace humble-ros-base",
  "remoteUser": "rosdev",
  "dockerComposeFile": "${localWorkspaceFolder}/docker-compose.yml",
  "service": "autonomous_system",
  "workspaceFolder": "/home/rosdev/ros2_ws",
  "workspaceMount": "source=${localWorkspaceFolder},target=/home/rosdev/ros2_ws/src,type=bind",
  "mounts": [],
  "runArgs": [
      "--net=host"
  ],
  "customizations": {
      "vscode": {
          "extensions": [
              "ms-python.python",
              "charliermarsh.ruff",
              "ms-vscode.cpptools-extension-pack",
              "ms-iot.vscode-ros",
              "redhat.vscode-xml",
              "tamasfe.even-better-toml",
              "yzhang.markdown-all-in-one",
          ],
          "settings": {
              "files.associations": {
                  "*.rviz": "yaml",
                  "*.srdf": "xml",
                  "*.urdf": "xml",
                  "*.xacro": "xml"
              },
              "remote.autoForwardPorts": false
          }
      }
  }
}
